        "mapping": [
          {
            "identity": "servd",
            "map": "n/a"
          }
        ]
      },
      {
        "id": "Organization.active",
        "path": "Organization.active",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Organization.name",
        "path": "Organization.name",
        "min": 1,
        "mustSupport": true,
        "mapping": [
          {
            "identity": "servd",
            "map": "./PrimaryAddress and ./OtherAddresses"
          }
        ]
      },
      {
        "id": "Organization.telecom",
        "path": "Organization.telecom",
        "mustSupport": true
